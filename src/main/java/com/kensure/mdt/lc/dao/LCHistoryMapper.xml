<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kensure.mdt.lc.dao.LCHistoryDao"><!--dsName="" saasAspect=""-->
	<!-- <cache readOnly="true"/> -->
    <sql id="allFields">
		id,
		busitype,
		entry_name as entryName,
		userid,
		bisiid,
		created_time as createdTime,
		audit_result as auditResult,
		audit_opinion as auditOpinion
    </sql>
    
	<sql id="insertFields">
		id,
		busitype,
		entry_name,
		userid,
		bisiid,
		created_time,
		audit_result,
		audit_opinion
    </sql>
	
	<insert id="insert" parameterType="com.kensure.mdt.lc.model.LCHistory">
		insert into lc_history (<include refid="insertFields"/>)
		values (
			#{id},
			#{busitype},
			#{entryName},
			#{userid},
			#{bisiid},
			#{createdTime},
			#{auditResult},
			#{auditOpinion}
		)
	</insert>
	
	<insert id="insertInBatch" parameterType="com.kensure.mdt.lc.model.LCHistory">
		insert into lc_history (<include refid="insertFields"/>) values 
		<foreach collection="list" item="item" index="index" separator=",">
			(
				#{item.id},
				#{item.busitype},
				#{item.entryName},
				#{item.userid},
				#{item.bisiid},
				#{item.createdTime},
				#{item.auditResult},
				#{item.auditOpinion}
			)
		</foreach>
	</insert>   		
	
    <sql id="whereClause">
		<if test="busitype != null">			
			busitype = #{busitype}			
		</if>
		<if test="entryName != null">			
			and entry_name = #{entryName}			
		</if>
		<if test="userid != null">			
			and userid = #{userid}			
		</if>
		<if test="bisiid != null">			
			and bisiid = #{bisiid}			
		</if>
		<if test="createdTime != null">			
			and created_time = #{createdTime}			
		</if>
		<if test="auditResult != null">			
			and audit_result = #{auditResult}			
		</if>
		<if test="auditOpinion != null">			
			and audit_opinion = #{auditOpinion}			
		</if>
	</sql>
    
	<select id="selectOne" resultType="com.kensure.mdt.lc.model.LCHistory" parameterType="Long" >
		select <include refid="allFields"/> 
		  from lc_history 
		 where id = #{id} 
	</select>
	
	<select id="selectByIds" parameterType="List" resultType="com.kensure.mdt.lc.model.LCHistory">
		select <include refid="allFields"/> from lc_history 
		where id in
		<foreach collection="list" item="item" open="(" close=")" separator=",">
			#{item}
		</foreach>
	</select>
		
	<select id="selectAll" resultType="com.kensure.mdt.lc.model.LCHistory">
		select <include refid="allFields"/> 
		from lc_history 
	</select>

	<select id="selectCount" resultType="long">
		select count(*) from lc_history 
	</select>
	
	<select id="selectCountByWhere" parameterType="Map" resultType="Long">
		select count(*) from lc_history 
		<where>
		    <include refid="whereClause"/>
		</where>
	</select>

	<select id="selectByWhere" parameterType="Map" resultType="com.kensure.mdt.lc.model.LCHistory">
		select <include refid="allFields"/>
		from lc_history 
		<where>
		    <include refid="whereClause"/>
		</where>				
		<if test="orderby != null">
			ORDER BY ${orderby}
		</if>
	</select>
	
	<sql id="setComn">		
		<if test="busitype != null">
			busitype = #{busitype},
		</if>
		<if test="entryName != null">
			entry_name = #{entryName},
		</if>
		<if test="userid != null">
			userid = #{userid},
		</if>
		<if test="bisiid != null">
			bisiid = #{bisiid},
		</if>
		<if test="createdTime != null">
			created_time = #{createdTime},
		</if>
		<if test="auditResult != null">
			audit_result = #{auditResult},
		</if>
		<if test="auditOpinion != null">
			audit_opinion = #{auditOpinion}
		</if>
	</sql>
	
	<update id="update" parameterType="com.kensure.mdt.lc.model.LCHistory">
		 update lc_history 
		    <set>
		    	<include refid="setComn"/>
			</set>
		  where id = #{id}
	</update>
	
	<update id="updateByMap" parameterType="Map">
		 update lc_history 
		    <set>
		    	<include refid="setComn"/>
			</set>
		  where id = #{id}
	</update>
	
	<delete id="delete">
		delete from lc_history where id = #{id}
	</delete>
	
	<delete id="deleteMulti" parameterType="List">
		delete from lc_history
		where id in
		<foreach collection="list" item="item" open="(" close=")" separator=",">
			#{item}
		</foreach>
	</delete>
	
	<delete id="deleteByWhere" parameterType="Map">
		delete from lc_history 
		<where>
		    <include refid="whereClause"/>
		</where>
	</delete>	

</mapper>    
